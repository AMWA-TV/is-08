{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Describes an outputs's parameters",
  "title": "Output parameter resource",
  "additionalProperties": false,
  "properties":{
    "numChannels":{
      "type": "integer"
    },
    "senderID":{
      "type": [
        "string",
        "null"
      ],
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[1-5][0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$"
    }
  }
}